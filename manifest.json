{

    "manifest_version": 2,
    "name": "Auto Dark Whatsapp",
    "version": "1.1",
  
    "description": "Automatically enables Whatsapp's own dark theme.",
  
    "icons": {
      "48": "icons/icon-48.png"
    },
  
    "content_scripts": [
      {
        "matches": ["*://web.whatsapp.com/*"],
        "js": ["autodark.js"]
      }
    ]
  
  }